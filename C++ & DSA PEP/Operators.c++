#include<bits/stdc++.h>
using namespace std;

int main(){
    int a = 5;
    int b = 10;
    int c = b - a;
    int d = a + b;
    int e = a * b;
    int f = a / b;
    int g = a % b;
    cout << "Addition is: "<<d<<endl;
    cout << "Subtraction is: "<<c<<endl;
    cout << "Multiplication is: "<<e<<endl;
    cout << "Division is: "<<f<<endl;
    cout << "Modulo is: "<<g<<endl;
}